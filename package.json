{
  "name": "telegram-send",
  "private": true,
  "version": "0.0.0",
  "source": "src/manifest.json",
  "scripts": {
    "prepare": "parcel build --config @parcel/config-webextension --log-level verbose --no-source-maps --reporter @parcel/reporter-bundle-analyzer",
    "watch": "parcel watch --config @parcel/config-webextension --host localhost",
    "format": "eslint --ignore-path .gitignore --fix . && prettier --ignore-path .gitignore --write .",
    "test": "tsc --noEmit && eslint --ignore-path .gitignore . && prettier --ignore-path .gitignore --check ."
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/client": "preact/compat/client",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions"
  ],
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "dependencies": {
    "preact": "^10.10.0",
    "swr": "^1.3.0",
    "tg-bot-client": "^0.1.1",
    "webext-typed-messages": "file:../webext-typed-messages",
    "webext-typed-storage": "file:../webext-typed-storage"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.6.2",
    "@parcel/reporter-bundle-analyzer": "^2.6.2",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/chrome": "^0.0.193",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "parcel": "^2.6.2",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sharp": "^0.29.3",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.3"
  }
}
